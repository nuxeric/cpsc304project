<!DOCTYPE html>
<html>
    <head>
        <%- include('../../layout/head.ejs') -%>
    </head>
    <body>
        <%- include('../../layout/header.ejs') -%>
        
        <main>
            <div class="hero">
              <h1><%= title %></h1>
      
              <table>
                <thead>
                  <tr>
                    <th>ID</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                  </tr>
                </thead>
                
                <tbody>
                    <% if (div_result) {%>
                      <% div_result.forEach(p => { %>
                        <tr>
                          <td><%= p.id %></td>
                          <td><%= p.firstName %></td>
                          <td><%= p.lastName %></td>
                        </tr>
                      <% }); %>
                    <% } %>
                  </tbody>

              </table>
      
            </div>
          </main>

        <%- include('../../layout/foot.ejs') -%>
    </body>
</html>